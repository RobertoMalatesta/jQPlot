{
    "source": {
        "include": ["src/core", "package.json", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|jsdate.js)"
    },
    "destination": "./docs/",
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/minami"
    }
}